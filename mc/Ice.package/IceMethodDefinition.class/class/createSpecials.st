private
createSpecials
	| map |
	
	map := Dictionary new
		at: $+ put: 'plus';
		at: $- put: 'minus';
		at: $= put: 'equals';
		at: $< put: 'less';
		at: $> put: 'more';
		at: $% put: 'percent';
		at: $& put: 'and';
		at: $| put: 'pipe';
		at: $* put: 'star';
		at: $/ put: 'slash';
		at: $\ put: 'backslash';
		at: $~ put: 'tilde';
		at: $? put: 'wat';
		at: $, put: 'comma';
		at: $@ put: 'at';
		yourself.
	map keys do: [ :key | map at: (map at: key) put: key ].
	^ map
